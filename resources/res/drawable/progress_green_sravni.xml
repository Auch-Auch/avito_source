<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dim_4"/>
            <gradient android:startColor="@color/progress_grey_sravni" android:endColor="@color/progress_grey_sravni" android:angle="270" android:centerY="1" android:centerColor="@color/progress_grey_sravni"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dim_4"/>
                <gradient android:startColor="@color/shamrock_green_sravni" android:endColor="@color/shamrock_green_sravni" android:angle="270" android:centerY="1" android:centerColor="@color/shamrock_green_sravni"/>
            </shape>
        </clip>
    </item>
</layer-list>
