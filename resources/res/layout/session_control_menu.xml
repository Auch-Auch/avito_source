<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:id="@+id/sessions_menu_root" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:id="@+id/not_me_item" android:padding="14dp" android:layout_width="match_parent" android:layout_height="match_parent">
        <ImageView android:id="@+id/not_me_icon" android:layout_width="24dp" android:layout_height="24dp" android:src="@drawable/ic_unsubscribe_24" app:tint="?attr/black"/>
        <TextView android:textColor="?attr/black" android:id="@+id/not_me_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginStart="@dimen/standard_padding" style="?attr/textLegacy3_body"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:id="@+id/logout_item" android:padding="14dp" android:layout_width="match_parent" android:layout_height="match_parent">
        <FrameLayout android:layout_width="24dp" android:layout_height="24dp">
            <ImageView android:id="@+id/logout_icon" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/ic_exit_session_24" app:tint="?attr/black"/>
            <ProgressBar android:id="@+id/logout_progress" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
        </FrameLayout>
        <TextView android:textColor="?attr/black" android:id="@+id/logout_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginStart="@dimen/standard_padding" style="?attr/textLegacy3_body"/>
    </LinearLayout>
</LinearLayout>
