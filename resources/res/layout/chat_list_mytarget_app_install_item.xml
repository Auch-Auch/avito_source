<?xml version="1.0" encoding="utf-8"?>
<com.my.target.nativeads.views.NativeAdContainer xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/nativeads_ad_view" android:background="@drawable/bg_white_selectable_blue_rectangle" android:paddingTop="10dp" android:paddingBottom="10dp" android:addStatesFromChildren="true" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="16dp" android:paddingEnd="16dp">
        <LinearLayout android:orientation="horizontal" android:id="@+id/chat_list_element" android:focusable="true" android:clickable="true" android:layout_width="match_parent" android:layout_height="wrap_content">
            <FrameLayout android:layout_width="48dp" android:layout_height="42dp" android:layout_marginTop="2dp" android:layout_marginEnd="10dp">
                <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/image" android:layout_width="match_parent" android:layout_height="match_parent" app:roundedCornerRadius="3dp"/>
                <com.avito.android.design.widget.ForegroundFrameLayout android:id="@+id/media_view_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
            </FrameLayout>
            <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.avito.android.messenger.widget.ChatListAdBannerTitleContainer android:id="@+id/title_block" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginRight="10dp">
                    <TextView android:textColor="?attr/black" android:id="@+id/nativeads_title" android:layout_width="wrap_content" android:layout_height="wrap_content" app:fontFamily="@font/avito_bold" style="?attr/textLegacy3_bodyDense"/>
                    <TextView android:id="@+id/nativeads_advertising" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/advertising_title" android:layout_marginStart="6dp" style="@style/res_2132017907_design_widget_badge_ad_single"/>
                    <TextView android:id="@+id/nativeads_age_restrictions" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="4dp" style="@style/res_2132017909_design_widget_badge_age_rds"/>
                </com.avito.android.messenger.widget.ChatListAdBannerTitleContainer>
                <Button android:id="@+id/nativeads_call_to_action" android:background="@null" android:layout_width="wrap_content" android:minHeight="20dp" style="@style/res_2132017260_avito_button_appinstall"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</com.my.target.nativeads.views.NativeAdContainer>
