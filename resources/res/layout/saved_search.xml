<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_gravity="top" android:id="@+id/saved_search_screen_root" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/content" android:layout_width="match_parent" android:layout_height="match_parent">
        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout android:id="@+id/swipe_refresh_layout" android:background="@color/expected_background" android:layout_width="match_parent" android:layout_height="match_parent">
            <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent">
                <androidx.recyclerview.widget.RecyclerView android:id="@+id/search_subscription_list" android:paddingTop="16dp" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent"/>
                <FrameLayout android:id="@+id/empty" android:background="?attr/white" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
                    <include layout="@layout/saved_search_empty"/>
                </FrameLayout>
            </FrameLayout>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </FrameLayout>
</FrameLayout>
