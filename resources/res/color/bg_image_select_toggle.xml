<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_enabled="false" android:state_checked="false" android:color="@color/common_white"/>
    <item android:state_enabled="false" android:state_checked="true" android:color="@color/common_gray_28"/>
